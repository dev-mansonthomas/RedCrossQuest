{
    "name": "rcq/redcrossquest",
    "description": "French Red Cross yearly fundraising rest backend",
    "keywords": ["RedCross","CroixRouge","FundRaising"],
    "homepage": "https://github.com/dev-mansonthomas/RedCrossQuest",
    "license": "GPL-3.0-or-later",
    "type": "project",
    "authors": [
        {
            "name": "Thomas Manson",
            "email": "cv@mansonthomas.com",
            "homepage": "http://www.mansonthomas.com/"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-PDO": "*",
        "ext-bz2": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "doctrine/annotations": "^1.12.1",
        "egulias/email-validator": "^2.1.25",
        "fzaninotto/faker": "^v1.9.2",
        "google/apiclient": "^v2.9.1",
        "google/cloud": "^v0.150.0",
        "google/protobuf": "^v3.15.2",
        "google/recaptcha": "^1.2.4",
        "grpc/grpc": "^v1.27.0",
        "guzzlehttp/guzzle": "^7.0",
        "jolicode/slack-php-api": "^2.5",
        "kreait/firebase-php": "^5.8.1",
        "lcobucci/jwt": "^3.4.5",
        "nesbot/carbon": "^2.45.1",
        "nyholm/psr7": "^1.4.0",
        "php-di/php-di": "^6.3.0",
        "phpmailer/phpmailer": "^v6.3.0",
        "ramsey/uuid": "^3.9.3",
        "robmorgan/phinx": "^0.12.5",
        "sendgrid/sendgrid": "^7.9.2",
        "slim/php-view": "^2.2.1",
        "slim/psr7": "^1.3.0",
        "slim/slim": "^4.7.1",
        "symfony/http-client": "^v5.2.3",
        "symfony/validator": "^v5.1.5",
        "zircote/swagger-php": "^3.1.0"
    },
    "autoload": {
      "psr-4": {"RedCrossQuest\\": "src/"}
    },

    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "7.4"
        }
    }
}
